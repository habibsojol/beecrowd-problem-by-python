import math 
n1 = list(map(float, input().split()))
x1, y1 = n1
n2 = list(map(float, input().split()))
x2, y2 = n2


d = pow((x2-x1), 2) + pow((y2-y1), 2)      #d = ((x2-x1)**2) + ((y2-y1)**2) we can write this line same this

s = math.sqrt(d)
print(f"{s:.4f}")   #print("%.4f" %s) we can write this line same this
