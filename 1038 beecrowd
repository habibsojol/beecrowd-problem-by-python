p, i = map(int, input().split())

n1 = 4
n2 = 4.50
n3 = 5
n4 = 2
n5 = 1.50

if (p==1):
  v = i * n1
  print(f"Total: R$ {v:.2f}")

elif (p==2):
  v = i * n2
  print(f"Total: R$ {v:.2f}")

elif (p==3):
  v = i * n3
  print(f"Total: R$ {v:.2f}")

elif (p==4):
  v = i * n4
  print(f"Total: R$ {v:.2f}")
      
elif (p==5):
  v = i * n5
  print(f"Total: R$ {v:.2f}")
